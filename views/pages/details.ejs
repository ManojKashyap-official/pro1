<style>
    td {
        padding-left: 22px;
    }
    
    th {
        padding-left: 16px;
    }
</style>
<div class="w3-container">
    <h2>Student details Table</h2>


    <table class="w3-table">
        <tr>
            <th>Student Id</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Gender</th>
            <th>DOB</th>
            <th>Address</th>
            <th>Education</th>
            <th>Board</th>
            <th>Passing</th>
            <th>Score</th>
            <th>interested Course</th>

        </tr>
        <% for(let i = 0; i < data.length; i++) { %>
            <tr>
                <td>
                    <%=data[i].id %>
                </td>
                <td>
                    <%=data[i].firstName %>
                </td>
                <td>
                    <%=data[i].lastName %>
                </td>
                <td>
                    <%=data[i].gender %>
                </td>
                <td>
                    <%=data[i].dob %>
                </td>
                <td>
                    <%=data[i].address %>
                </td>
                <td>
                    <%=data[i].education %>
                </td>
                <td>
                    <%=data[i].board %>
                </td>
                <td>
                    <%=data[i].passing %>
                </td>
                <td>
                    <%=data[i].score %>
                </td>
                <td>
                    <%=data[i].course %>
                </td>
            </tr>
            <%}%>
    </table>

    <a href="#">  Search Student</a>

    <a href="/deleteStudent"> Delete Student</a>
    <a href="/update"> Update Student Details.</a>
    <a href="/" role="button">back</a>

</div>